{
  "service": "s3",
  "method": "putObject",
  "params": {
    "ContentType": "application/json",
    "Bucket": "myBucket",
    "Key": "hello.json",
    "Body": {
      "Hello": "World"
    }
  },
  "outputFilters": [
    {
      "path": "Body",
      "filter": "parse",
      "type": "json"
    }
  ]
}